# MCP Server Credentials Configuration
#
# To use this file:
# 1. Copy it to config/credentials.toml
# 2. Update the API keys below with your own secure keys
# 3. Add config/credentials.toml to .gitignore (DO NOT commit it!)
#
# Each section represents a user, with their API key and optional external service credentials

[alice]
api_key = "alice-secret-key-12345"

[alice.external_keys]
# Add external service credentials for this user here
# example_api_key = "your-api-key-here"
# postgres_url = "postgresql://localhost/alicedb"

[bob]
api_key = "bob-secret-key-67890"

[bob.external_keys]
